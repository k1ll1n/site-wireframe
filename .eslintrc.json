{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "google"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "semi": ["error", "never"],
        "quotes": ["error", "single"],
        "no-var": "error",
        "comma-dangle": ["error", "never"],
        "eol-last": ["error", "never"]
    }
}
